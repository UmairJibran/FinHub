# Production Environment Configuration
# This file contains production-specific environment variables

# Supabase Configuration
VITE_SUPABASE_URL=your_production_supabase_url
VITE_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# Analytics Configuration
VITE_UMAMI_WEBSITE_ID=your_umami_website_id

# Performance Configuration
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ANALYTICS=true

# Security Configuration
VITE_ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Feature Flags
VITE_ENABLE_OFFLINE_SUPPORT=true
VITE_ENABLE_REAL_TIME_SYNC=true

# Build Configuration
VITE_BUILD_ANALYZE=false
VITE_BUILD_SOURCEMAP=false

# Cache Configuration
VITE_CACHE_VERSION=1
VITE_SW_CACHE_NAME=finhub-v1

# API Configuration
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3