# Example environment variables
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
# Get these from your Supabase project dashboard > Settings > API
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Security Configuration
# Comma-separated list of allowed origins for CORS (production only)
VITE_ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Analytics (optional)
VITE_UMAMI_WEBSITE_ID=your-umami-id-here

# Performance and Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=true

# Development flags (optional)
VITE_ENABLE_SECURITY_TESTING=true
VITE_ENABLE_RLS_TESTING=true

# Production optimizations
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_PRELOAD_HINTS=true